x<-seq(-pi,pi,len=50)
y<-x
f<-outer(x,y,function(x,y) cos(y)/1+x^2)
oldpar<-par(no.readonly=TRUE)
par(pty="s")
contour(x,y,f)
contour(x,y,f,nlevels=15,add=true)
contour(x,y,f,nlevels=15,add=TRUE)
fa<-(f-t(f))/2
contour(x,y,fa,nlevels=15)
par(oldpar)
image(x,y,f)
image(x,y,fa)
objects();rm(x,y,f,fa)
